page{
    width: 100%;
    height: 100%;
}
.container{
    position: relative;
    width: 100%;
    height: 100%;
}
.container .header_wrap{
    height: 162rpx;
    width: 100%;
    background: #F5F5F5;
    box-sizing: border-box;
    padding: 40rpx 30rpx 0 30rpx;
}
.header_wrap .header_bar{
    position: relative;
    width: 100%;
    height: 72rpx;
    display: flex;
    justify-content: space-between;
}
.header_bar .sort{
    height: 72rpx;
    line-height: 72rpx;
    padding: 0 27rpx;
    background:rgba(255,255,255,1);
    box-shadow:0px 0px 10rpx 0px rgba(0,0,0,0.1);
    border-radius:4rpx;
    font-size:30rpx;
    font-family:PingFangSC-Medium;
    font-weight:500;
    color:rgba(51,51,51,1);
}
.sort.act{
    color:rgba(255,255,255,1);
    background:linear-gradient(194deg,rgba(0,208,126,1) 0%,rgba(0,146,174,1) 100%);
    box-shadow:0px 0px 10px 0px rgba(43,123,93,0.5);
}

.search_wrap{
    position: fixed;
    bottom: 120rpx;
    right: 20rpx;
    z-index: 588;
    width: 80rpx;
    height: 200rpx;
}
.search_wrap .sacn{
    position: absolute;
    top: 0;
    right: 0;
    width: 80rpx;
    height: 80rpx;
}
.sacn image{
    width: 100%;
}
.search_wrap .search{
    position: absolute;
    right:0;
    bottom: 0;
    width: 80rpx;
    height: 80rpx;
}
.search image{
    width: 100%;
}

/* 内容区 */
.content{
    position: absolute;
    left: 0;
    top:142rpx;
    bottom: 0;
    width: 100%;
    background:rgba(255,255,255,1);
    border-radius:20rpx 20rpx 0px 0px;
}
.content .swiper{
    position: relative;
    width: 100%;
    height: 100%;
}
.swipe swiper-item{
    background: #fff;
}
.swiper .slide_con{
    width: 100%;
    height: 100%;
}
.slide_con .no_data{
    position: relative;
    width: 100%;
    height: 100%;
}
.no_data .empty{
    position: absolute;
    left: 197rpx;
    top: 252rpx;
    width: 360rpx;
}
.list{
    position: relative;
    width: 100%;
    padding-top: 40rpx;
    padding-left: 30rpx;
    box-sizing: border-box;
    overflow: hidden;
}
.list .item{
    position: relative;
    float: left;
    width: 160rpx;
    margin: 0 35rpx 50rpx;
}
.item .img_wrap{
    position: relative;
    width:160rpx;
    height:160rpx;
    border-radius: 50%;
    border:4rpx solid rgba(255,255,255,1);
    box-sizing: border-box;
    overflow: hidden;
}
.img_wrap image{
    width: 100%;
}
.item .time{
    width:110rpx;
    height:30rpx;
    background:rgba(0,0,0,0.6);
    border-radius:15rpx;
    font-size:20rpx;
    font-family:PingFangSC-Medium;
    font-weight:500;
    color:rgba(255,255,255,1);
    text-align: center;
    margin: 10rpx auto 0;
}

.shade{
    position: fixed;
    left: 0;
    top: 0;
    z-index: 600;
    width: 100%;
    height: 100%;
    background:rgba(0,0,0,0.7);
}
.shade_center{
    position: fixed;
    left: 50%;
    top: 50%;
    z-index: 688;
    transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    width: 480rpx;
    overflow: hidden;
}
.shade_center .shareimg_w{
    position: relative;
    width: 100%;
    overflow: hidden;
}
.shareimg_w .img{
    width: 100%;
    max-height: 1300rpx;
}
.shareimg_w .img_msg{
    position: absolute;
    left: 0;
    bottom: 0;
    width:480rpx;
    height:200rpx;
    font-size:30rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(255,255,255,1);
    line-height:50rpx;
    padding-top: 120rpx;
    box-sizing: border-box;
    text-align: center;
    background:linear-gradient(180deg,rgba(0,0,0,0) 0%,rgba(0,0,0,0.63) 46%,rgba(0,0,0,1) 100%);
}

.shade_center .share_btn{
    position: relative;
    width: 340rpx;
    height: 110rpx;
    margin: 50rpx auto 0
}
.btn_bg{
    width: 100%;
}
.share_btn .sahre{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 58;
    opacity: 0;
}



