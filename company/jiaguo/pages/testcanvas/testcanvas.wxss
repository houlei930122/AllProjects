/* pages/testcans/testcanvas.wxss */
@font-face {
    font-family: 'cfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2');
    font-weight: normal;
    font-style: normal;
}

.test{
    position: fixed;
    top: -100px;  
    z-index: 10000;
    font-size: 50rpx;
    font-family: 'cfont';
    color: #fff;
    
}




.container{
    position: relative;
    width: 100%;
    height: 100%;
}
.share_wrap{
    position: relative;
    width: 100%;
    /* padding: 0 60rpx; */
}
.img{
    width: 100%;
}
.share_wrap .share_btnw{
    position: fixed;
    left: 0;
    bottom: 0;
    width:100%;
    height:326rpx;
    background:linear-gradient(180deg,rgba(0,0,0,0) 0%,rgba(0,0,0,1) 50%,rgba(0,0,0,1) 100%);
    z-index: 8;
}
.share_btnw .saveimg{
    position: absolute;
    bottom: 40rpx;
    left: 100rpx;
    width:236rpx;
    height:68rpx;
    line-height:68rpx;
    background:linear-gradient(240deg,rgba(235,172,42,1) 0%,rgba(255,225,101,1) 100%);
    border-radius:37rpx;
    text-align: center;
    font-size:28rpx;
    font-family:MicrosoftYaHei;
    color:rgba(51,51,51,1);
}
.share_btnw .sharev{
    position: absolute;
    bottom: 40rpx;
    right: 100rpx;
    width:236rpx;
    height:68rpx;
    line-height:68rpx;
    background:linear-gradient(240deg,rgba(235,172,42,1) 0%,rgba(255,225,101,1) 100%);
    border-radius:37rpx;
    text-align: center;
    font-size:28rpx;
    font-family:MicrosoftYaHei;
    color:rgba(51,51,51,1);
}


